{
    "project": {
        "version": "1.0.0-beta.22-1",
        "name": "mugloar"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "favicon.ico"
            ],
            "index": "index.html",
            "main": "main.ts",
            "tsconfig": "tsconfig.json",
            "prefix": "mugloar",
            "mobile": false,
            "styles": [
                "styles.styl",
                "theme.scss"
            ],
            "scripts": [
            ],
            "environments": {
                "source": "environments/environment.ts",
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "addons": [
        "assets/fonts/*.+(otf|eot|svg|ttf|woff)"
    ],
    "packages": [
    ],
    "defaults": {
        "styleExt": "styl",
        "prefixInterfaces": false,
        "inline": {
            "style": false,
            "template": false
        },
        "spec": {
            "class": false,
            "component": false,
            "directive": false,
            "module": false,
            "pipe": false,
            "service": false
        }
    }
}